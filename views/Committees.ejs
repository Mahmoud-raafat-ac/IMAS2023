      </table>

      <h2>Women in Engineering Committee</h2>
      <table class="table">
          <thead class="thead-dark">
          <tr>
              <th scope="col">#</th>
              <th scope="col">Name</th>
          </tr>
          </thead>
          <tbody>
          <%for(let i = 0;i<women.length;i++){%>
          <tr>
              <th scope="row"><%=i+1%></th>
              <td><%=women[i].name%></td>
          </tr>
          <%}%>
