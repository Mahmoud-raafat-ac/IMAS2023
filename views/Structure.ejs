<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/bootstrap.min.css">
    <link rel="stylesheet" href="css/all.min.css">
    <link rel="stylesheet" href="css/aos.css">
    <link rel="stylesheet" href="css/main.css">
    <link rel="icon" href="imgs/logo/logo.png">
    <title>IMAS 2023</title>
    <style>
        h2{
            margin-top: 50px;
            text-align: center;
            margin-bottom: 20px;
        }
    </style>
</head>
<body>
    <!-- main header -->
    <div class="bg"></div>
    <!-- /main-header -->

    <!-- header-menu -->
    <div class="container-fluid header-menu">
    <div class="container">
        <div class="links">
        <a href="/" class="link">Home</a>
        <a href="/About" class="link">About</a>
        <a href="/Agenda" class="link">Agenda</a>
        <a href="/Topics" class="link">Topics</a>
        <a href="/Structure" class="link">Structure</a>
        <a href="/Sponsers" class="link">Sponsers</a>
        <a href="/Country" class="link">Country</a>
        </div>
    </div>
    </div>
    <!-- /header-menu -->

    <!-- main -->
    <div style="margin-bottom: 200px" class="container">
        <h2>Oversight Committee</h2>
        <table class="table">
            <thead class="thead-dark">
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Email</th>
                <th scope="col">Role</th>
            </tr>
            </thead>
            <tbody>
            <%for(let i = 0;i<over.length;i++){%>
            <tr>
                <th scope="row"><%=i+1%></th>
                <td><%=over[i].name%></td>
                <td><%=over[i].email%></td>
                <td><%=over[i].role%></td>
            </tr>
            <%}%>
            </tbody>
        </table>
        <h2>Steering Committee</h2>
        <table class="table">
            <thead class="thead-dark">
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Role</th>
            </tr>
            </thead>
            <tbody>
            <%for(let i = 0;i<steering.length;i++){%>
            <tr>
                <th scope="row"><%=i+1%></th>
                <td><%=steering[i].name%></td>
                <td><%=steering[i].role%></td>
            </tr>
            <%}%>
            </tbody>
        </table>
        <h2>Organizing Committee </h2>
        <table class="table">
            <thead class="thead-dark">
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Role</th>
            </tr>
            </thead>
            <tbody>
            <%for(let i = 0;i<organizing.length;i++){%>
            <tr>
                <th scope="row"><%=i+1%></th>
                <td><%=organizing[i].name%></td>
                <td><%=organizing[i].role%></td>
            </tr>
            <%}%>
            </tbody>
        </table>

        <h2>Finance Committee </h2>
        <table class="table">
            <thead class="thead-dark">
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Role</th>
            </tr>
            </thead>
            <tbody>
            <%for(let i = 0;i<finance.length;i++){%>
            <tr>
                <th scope="row"><%=i+1%></th>
                <td><%=finance[i].name%></td>
                <td><%=finance[i].role%></td>
            </tr>
            <%}%>
            </tbody>
        </table>

        <h2>Workshops, Tutorials, special sessions & Exhibition
        </h2>
        <table class="table">
            <thead class="thead-dark">
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Role</th>
            </tr>
            </thead>
            <tbody>
            <%for(let i = 0;i<workshops.length;i++){%>
            <tr>
                <th scope="row"><%=i+1%></th>
                <td><%=workshops[i].name%></td>
                <td><%=workshops[i].role%></td>
            </tr>
            <%}%>
            </tbody>
        </table>

        <h2>Women in Eng. Group</h2>
        <table class="table">
            <thead class="thead-dark">
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Role</th>
            </tr>
            </thead>
            <tbody>
            <%for(let i = 0;i<women.length;i++){%>
            <tr>
                <th scope="row"><%=i+1%></th>
                <td><%=women[i].name%></td>
                <td><%=women[i].role%></td>
            </tr>
            <%}%>
            </tbody>
        </table>
    </div>






    <!-- /main -->

    <!-- Footer -->
    <div class="container-fluid footer">
        <div class="container">
          <span>© Copyright 2022 IEEECUSB – All rights reserved.  A not-for-profit organization, IEEE is the world's largest technical professional organization dedicated to advancing technology for the benefit of humanity.</span>
          <div class="links">
            <a href="http://www.ieee.org/" class="link">IEEE</a>|
            <a href="http://ieeexplore.ieee.org/" class="link">IEEE Xplore</a>|
            <a href="http://standards.ieee.org/" class="link">IEEE Standards</a>|
            <a href="http://spectrum.ieee.org/" class="link">IEEE Spectrum</a>|
            <a href="http://www.ieee.org/sitemap" class="link d-md-inline-block d-none">More sites</a>
          </div>
        </div>
      </div>
      <!-- /Footer -->
  
      <script src="js/bootstrap.bundle.min.js"></script>
      <script src="js/aos.js"></script>
      <script>
        AOS.init();
      </script>
  </body>
  </html>
